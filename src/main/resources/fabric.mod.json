{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name} [C] [F]",
  "description": "${mod_description}",
  "authors": [
    "${mod_authors}"
  ],
  "contact": {
    "homepage": "${url_homepage}",
    "sources": "${git_sources}",
    "issues": "${git_issues}"
  },
  "license": "${mod_license}",
  "icon": "logo.png",
  "environment": "*",
  "mixins": [
    "pigs_have_litters.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": ">=${minecraft_version}"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.website": "${url_homepage}"
      }
    }
  }
}